// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_DATABASE_URL")
}

model Page {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  page  String
  url   String

  @@map("Pages")
}