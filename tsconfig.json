{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es6",
		"allowJs": true,
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true
	},
	"include": [
		"./back-end/**/*.ts",
		"./front-end/**/*.ts",
		"back-end/widgets/homepage/main-carousel.js"
	],
	"exclude": ["node_modules"],
	"types": ["express"]
}
